{
  "manifest_version": 2,

  "name": "reCaptcha Solver",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["jquery.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "background",
    "*://*.google.com/*"
  ]
}
